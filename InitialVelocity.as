package com{	public class InitialVelocity {		private var INITIAL_VELOCITY:Number;//m/s		private var INITIAL_DEGREES:Number;//degrees		//initial velocity , initial degrees at velocity		public function InitialVelocity(INITIAL_V:Number=5, INITIAL_D:Number=90) {			INITIAL_VELOCITY=INITIAL_V;			INITIAL_DEGREES=INITIAL_D;			trace("created");		}				private function degToRadian(deg:Number):Number {			return deg * Math.PI / 180;		}		public function f_initial_vx():Number {			var initial_v_x:Number;			initial_v_x = INITIAL_VELOCITY*Math.cos(degToRadian(INITIAL_DEGREES));			return initial_v_x;		}		public function f_initial_vy():Number {			var initial_v_y:Number= INITIAL_VELOCITY*Math.sin(degToRadian(INITIAL_DEGREES));			return initial_v_y;		}	}}